{
  "version": 1,
  "projects": {
    "weekee": {
      "root": "libs/weekee",
      "sourceRoot": "libs/weekee/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["libs/weekee/tsconfig.lib.json"],
            "exclude": ["**/node_modules/**", "!libs/weekee/**"]
          }
        }
      }
    },
    "may-epp": {
      "root": "apps/may-epp",
      "sourceRoot": "apps/may-epp/src",
      "projectType": "application",
      "prefix": "may-epp",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/may-epp",
            "main": "apps/may-epp/src/main.ts",
            "tsConfig": "apps/may-epp/tsconfig.app.json",
            "assets": ["apps/may-epp/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/may-epp/src/environments/environment.ts",
                  "with": "apps/may-epp/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "may-epp:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/may-epp/tsconfig.app.json"],
            "exclude": ["**/node_modules/**", "!apps/may-epp/**"]
          }
        }
      }
    },
    "wiki-graphql": {
      "root": "libs/wiki-graphql",
      "sourceRoot": "libs/wiki-graphql/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/wiki-graphql/.eslintrc",
            "tsConfig": ["libs/wiki-graphql/tsconfig.lib.json"],
            "exclude": ["**/node_modules/**", "!libs/wiki-graphql/**"]
          }
        }
      }
    },
    "wiki-bacon": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/wiki-bacon",
      "sourceRoot": "apps/wiki-bacon/src",
      "prefix": "wiki",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/wiki-bacon",
            "index": "apps/wiki-bacon/src/index.html",
            "main": "apps/wiki-bacon/src/main.ts",
            "polyfills": "apps/wiki-bacon/src/polyfills.ts",
            "tsConfig": "apps/wiki-bacon/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/wiki-bacon/src/favicon.ico",
              "apps/wiki-bacon/src/assets"
            ],
            "styles": ["apps/wiki-bacon/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/wiki-bacon/src/environments/environment.ts",
                  "with": "apps/wiki-bacon/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "wiki-bacon:build",
            "proxyConfig": "apps/wiki-bacon/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "wiki-bacon:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wiki-bacon:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/wiki-bacon/tsconfig.app.json",
              "apps/wiki-bacon/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/wiki-bacon/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/wiki-bacon/jest.config.js",
            "tsConfig": "apps/wiki-bacon/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/wiki-bacon/src/test-setup.ts"
          }
        }
      }
    },
    "wiki-bacon-e2e": {
      "root": "apps/wiki-bacon-e2e",
      "sourceRoot": "apps/wiki-bacon-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/wiki-bacon-e2e/cypress.json",
            "tsConfig": "apps/wiki-bacon-e2e/tsconfig.e2e.json",
            "devServerTarget": "wiki-bacon:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wiki-bacon:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/wiki-bacon-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/wiki-bacon-e2e/**"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/node"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "may-epp"
}
