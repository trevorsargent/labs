  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Weekee Server - Attach",
      "port": 9876,
      "skipFiles": ["<node_internals>/**"],
      "restart": true
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Wiki Bacon - Chrome",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/apps/wiki-bacon"
    }
  ]
}
